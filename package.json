{
  "name": "airborne",
  "version": "0.0.0-semantically-released",
  "description": "Airborne RESTful API framework",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha tests",
    "test:watch": "mocha tests -w",
    "lint": "eslint src",
    "start": "eslint src && babel -w src -d dist",
    "build": "eslint src && babel src -d dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niklucky/airborne.git"
  },
  "keywords": [
    "ariborne",
    "engine",
    "framework",
    "api",
    "dao",
    "mapper",
    "controller",
    "service",
    "model",
    "schema",
    "mysql",
    "redis",
    "database"
  ],
  "files": [
    "dist",
    "docs",
    "examples",
    "README.md"
  ],
  "author": "Nikita Zhuravlev",
  "license": "MIT",
  "homepage": "https://github.com/niklucky/airborne#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^2.13.1",
    "ghooks": "^1.3.2",
    "mocha": "^2.5.3",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lodash": "^4.13.1",
    "mysql": "^2.11.1",
    "mysql-qb": "^0.6.2",
    "redis": "^2.6.2",
    "sanitizer": "^0.1.3"
  },
  "engines": {
    "node": ">=4.1.2 =<6.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
