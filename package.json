{
  "name": "airborne",
  "description": "Airborne RESTful API framework",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 80 --branches 80 --functions 80 --lines 80",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha test --recursive --compilers js:babel-register",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t",
    "lint": "eslint src",
    "build:watch": "eslint src && babel -w src -d dist",
    "prebuild": "rimraf dist",
    "build": "eslint src && babel src -d dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niklucky/airborne.git"
  },
  "keywords": [
    "ariborne",
    "engine",
    "framework",
    "api",
    "dao",
    "mapper",
    "controller",
    "service",
    "model",
    "schema",
    "mysql",
    "redis",
    "database"
  ],
  "files": [
    "dist",
    "docs",
    "examples",
    "README.md"
  ],
  "author": "Nikita Zhuravlev",
  "license": "MIT",
  "homepage": "https://github.com/niklucky/airborne#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "nyc": "^8.3.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lodash": "^4.13.1",
    "mongodb": "^2.2.10",
    "mysql": "^2.11.1",
    "mysql-qb": "^0.x.x",
    "redis": "^2.6.2",
    "sanitizer": "^0.1.3"
  },
  "engines": {
    "node": ">=4.1.2 =<6.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "version": "0.0.0-development"
}
